<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.apphive.doordekhaetui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ActSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ActDashboard"
            android:configChanges="orientation|screenSize|keyboard|keyboardHidden"></activity>

        <service android:name=".service.ServiceAdaptHostingURL" ></service>
        <service android:name=".service.ServiceAdaptChannelURL" ></service>

        <receiver android:name=".receiver.ReceiverHostResponse">
            <intent-filter>
                <action android:name="com.rz.receive.action.urlresponse.RECEIVE_MESSAGE_ADAPT_HOST_URL_INDEXING" />
                <action android:name="com.rz.receive.action.urlresponse.RECEIVE_MESSAGE_ADAPT_CHANNEL_URL_INDEXING" />
            </intent-filter>
        </receiver>
        <service android:name="com.rz.librarycore.admob.ServiceAdMobInterstitial"></service>
    </application>

</manifest>